# .env.example
# Copy this file to .env and fill in your actual values.

# Google OAuth Credentials
GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID"
GOOGLE_CLIENT_SECRET="YOUR_GOOGLE_CLIENT_SECRET"

# Secret key for session management (generate a strong, random string)
SECRET_KEY="YOUR_VERY_STRONG_RANDOM_SECRET_KEY"

# Redirect URI for Google OAuth (e.g., http://localhost:8000/auth/google)
# This must match the Authorized redirect URIs configured in your Google Cloud Console.
REDIRECT_URI="http://localhost:8000/auth/google"
